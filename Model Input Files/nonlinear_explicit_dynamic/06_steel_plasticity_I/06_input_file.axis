TITLE "NON-LINEAR DYNAMIC EXPLICIT SOLVER TEST SERIES -- TEST 3: RUBBER BENDING LOAD TEST I"

## Analysis settings
BEGIN ANALYSIS
	BEGIN STEP WITH NAME = "Transient analysis with no hourglass control",
					TYPE = EXPLICIT_STANDARD, START_TIME = 0, END_TIME = 1e-2, 
					DTIME_CONTROL_TYPE = DTIME_CONSTANT, 
					DTIME_CONTROL_PARAMS = (DTIME = 2e-6)
/*	BEGIN STEP WITH NAME = "Transient analysis with no hourglass control",
					TYPE = EXPLICIT_STANDARD, START_TIME = 0, END_TIME = 5E-2, 
					DTIME_CONTROL_TYPE = EXPLICIT_TIME_CONTROL, 
					DTIME_CONTROL_PARAMS = (DTIME_SCALE_FACTOR = 0.02)*/
		BEGIN SNAPSHOTS
			DO 250 SNAPSHOTS
		END SNAPSHOTS

		## HYPERWORKS RESULT FILE
		BEGIN OUTPUT WITH FILE = "plasticity_results", FORMAT = HYPERWORKS
			RECORD NODAL DISPLACEMENT
			RECORD ELEMENT STRESS 
			RECORD ELEMENT PLASTIC_STRAIN
			RECORD ELEMENT EFFECTIVE_PLASTIC_STRAIN
		END OUTPUT
		
		## MATLAB RESULT FILE
		BEGIN OUTPUT WITH FILE = "plasticity_results", 
		                  FORMAT = MATLAB_DATASET, 
						  FORMAT_PARAMS = (NAME = plastic_test)
			# Free end field output
			RECORD AVERAGE NODAL LOAD Y ON SET tip
			RECORD AVERAGE NODAL DISPLACEMENT Y ON SET tip
			RECORD AVERAGE ELEMENT STRESS XX,YY,XY ON SET tip_element
			RECORD AVERAGE NODAL VELOCITY Y ON SET tip
			RECORD AVERAGE NODAL ACCELERATION Y ON SET bottom_tip
			RECORD AVERAGE ELEMENT EFFECTIVE_PLASTIC_STRAIN ON SET tip_element
			RECORD AVERAGE ELEMENT PLASTIC_STRAIN ON SET tip_element
			RECORD AVERAGE ELEMENT STRAIN ON SET tip_element
		END OUTPUT
		BEGIN LOADS
			BEGIN NODAL_LOADS
				ON SET tip BEHAVES AS load_curve ON Y DIRECTION SCALE = -2000
			END NODAL_LOADS
		END LOADS
		BEGIN CONSTRAINTS
			LOCK SET encastre IN ALL DIRECTIONS
			LOCK SET tip IN X DIRECTION
		END CONSTRAINTS		
	END STEP
END ANALYSIS

