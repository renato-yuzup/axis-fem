TITLE "STATIC SOLVER TEST SERIES -- TEST 1: UNITY CUBE WITH POISSON = 0"

## Analysis settings
BEGIN ANALYSIS
#	BEGIN STEP WITH TYPE=LINEAR_STATIC, START_TIME = 0, END_TIME = 1, DTIME_CONTROL_TYPE = DTIME_CONSTANT, DTIME_CONTROL_PARAMS = (DTIME = 1)
	BEGIN STEP WITH TYPE=EXPLICIT_STANDARD, START_TIME = 0, END_TIME = 1, DTIME_CONTROL_TYPE = EXPLICIT_TIME_CONTROL, DTIME_CONTROL_PARAMS = (DTIME_SCALE_FACTOR = 0.8)
		BEGIN SNAPSHOTS
			SNAPSHOT AT 1
		END SNAPSHOTS

		BEGIN OUTPUT WITH FILE = "test_1", FORMAT = AXIS
			RECORD DISPLACEMENT ALL ON all_nodes
			RECORD STRAIN AT NODES ON SET all_nodes
			RECORD STRAIN AT ELEMENTS ON SET all_elements
			RECORD STRESS AT NODES ON SET all_nodes
			RECORD STRESS AT ELEMENTS ON SET all_elements
		END OUTPUT
		BEGIN LOADS
			BEGIN NODAL_LOADS
				ON SET top_face BEHAVES AS constant_curve ON Z DIRECTION
			END NODAL_LOADS
		END LOADS
		BEGIN CONSTRAINTS
			BLOCK MOVEMENT OF bottom_face ON ALL DIRECTIONS
			APPLY VELOCITY ON SET node_set IN ALL DIRECTIONS FOLLOWING curve_id
		END CONSTRAINTS		
	END STEP
END ANALYSIS

/* Our nodes and elements are declared apart in another file. */
BEGIN NODES WITH SET_ID = all_nodes
	1, 0.0, 0.0, 0.0
	2, 1.0, 0.0, 0.0
	3, 1.0, 1.0, 0.0
	4, 0.0, 1.0, 0.0
	5, 0.0, 0.0, 1.0
	6, 1.0, 0.0, 1.0
	7, 1.0, 1.0, 1.0
	8, 0.0, 1.0, 1.0
END NODES

BEGIN ELEMENTS WITH SET_ID = all_elements
	1: 1,2,3,4,5,6,7,8
END ELEMENTS

BEGIN PARTS WITH ELEM_TYPE = LINEAR_HEXAHEDRON, PROPERTIES = ()
	SET all_elements IS LINEAR_ISO_ELASTIC WITH POISSON = 0, ELASTIC_MODULUS = 200E9
END PARTS

BEGIN NODE_SET WITH ID = bottom_face
	1-4
END NODE_SET

BEGIN NODE_SET WITH ID = top_face
	5-8
END NODE_SET

BEGIN CURVES
	BEGIN MULTILINE_CURVE WITH ID = constant_curve
		0 -12500
		1 -12500
	END MULTILINE_CURVE
	
	BEGIN MULTILINE_CURVE WITH ID = pulse_curve
		0 		0
		0.001	1
		0.005	0
		100		0
	END MULTILINE_CURVE
END CURVES


