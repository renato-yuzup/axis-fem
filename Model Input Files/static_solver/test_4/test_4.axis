BEGIN PARTS WITH ELEM_TYPE = LINEAR_HEXAHEDRON, PROPERTIES = ()
	SET all_elements IS LINEAR_ISO_ELASTIC WITH POISSON = 0.3, ELASTIC_MODULUS = 200E9
END PARTS

BEGIN CONSTRAINTS
	BLOCK MOVEMENT OF clamped ON ALL DIRECTIONS
	BLOCK MOVEMENT OF constrained ON Y,Z DIRECTIONS
	BLOCK MOVEMENT OF support ON Z DIRECTION
END CONSTRAINTS

BEGIN LOADS
	BEGIN NODAL_LOADS
		ON SET carregamento1 BEHAVES AS constant_curve1 ON Z DIRECTION
		ON SET carregamento2 BEHAVES AS constant_curve2 ON Z DIRECTION
		ON SET carregamento3 BEHAVES AS constant_curve3 ON Z DIRECTION
	END NODAL_LOADS
END LOADS

BEGIN CURVES
	BEGIN MULTILINE_CURVE WITH ID = constant_curve1
		0 -10.0e3
		1 -10.0e3
	END MULTILINE_CURVE
	BEGIN MULTILINE_CURVE WITH ID = constant_curve2
		0 -20.0e3
		1 -20.0e3
	END MULTILINE_CURVE
	BEGIN MULTILINE_CURVE WITH ID = constant_curve3
		0 -40.0e3
		1 -40.0e3
	END MULTILINE_CURVE
END CURVES




 
